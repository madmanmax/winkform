{
    "name": "winkbrace/winkform",
    "type": "library",
    "description": "Forms library for PHP >= 5.3 using Laravel 4 Validation",
    "keywords": ["form", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bas de Ruiter",
            "email": "info@basderuiter.nl"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/translation": "4.1.x",
        "illuminate/validation": "4.1.x"
    },
    "require-dev": {
        "mockery/mockery": "0.7.2",
        "phpunit/phpunit": "3.7.*",
        "codeception/codeception": "1.6.*"
    },
    "autoload": {
        "psr-0": {
            "WinkForm": "src"
        }
    },
    "target-dir": "WinkForm",
    "minimum-stability": "dev"
}
