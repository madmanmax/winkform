{
    "name": "winkbrace/winkform",
    "type": "library",
    "description": "Forms library for PHP >= 5.3 using Laravel 4 Validation",
    "keywords": ["form", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bas de Ruiter",
            "email": "info@basderuiter.nl"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/translation": "~4.1",
        "illuminate/validation": "~4.1"
    },
    "require-dev": {
        "mockery/mockery": "~0.7",
        "phpunit/phpunit": "~3.7",
        "codeception/codeception": "~1.6"
    },
    "autoload": {
        "psr-0": {
            "WinkForm": "src"
        },
        "files": ["src/WinkForm/helpers.php"]
    },
    "minimum-stability": "dev"
}
